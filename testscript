if game:GetService("RbxAnalyticsService") then
    local clientId = game:GetService("RbxAnalyticsService"):GetClientId() 

local WindUI = loadstring(game:HttpGet("https://github.com/Footagesus/WindUI/releases/latest/download/main.lua"))()

WindUI:Popup({
    Title = "XYLO HUB | ClientID",
    Icon = "info",
    Content = clientId,
    Buttons = {
        {
            Title = "Cancel",
            Callback = function() end,
            Variant = "Tertiary",
        },
        {
            Title = "Copy",
            Icon = "square",
            Callback = function() 

     setclipboard(clientId)

WindUI:Notify({
    Title = "Copied ClientID",
    Content = "Succesfully copied the ClientID, you can now paste it in the discord",
    Duration = 5, -- 5 seconds
    Icon = "square",
})

end,
            Variant = "Primary",
        }
    }
})