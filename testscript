-- Korrigierte Version mit pcall
local success, analyticsService = pcall(function()
    return game:GetService("RbxAnalyticsService")
end)

if success then
    local clientId = analyticsService:GetClientId() 

    local WindUI = loadstring(game:HttpGet("https://github.com/Footagesus/WindUI/releases/latest/download/main.lua"))()

    WindUI:Popup({
        Title = "XYLO HUB | ClientID",
        Icon = "info",
        Content = clientId,
        Buttons = {
            {
                Title = "Cancel",
                Callback = function() end,
                Variant = "Tertiary",
            },
            {
                Title = "Copy",
                Icon = "square",
                Callback = function() 
                    setclipboard(clientId)
                    WindUI:Notify({
                        Title = "Copied ClientID",
                        Content = "Succesfully copied the ClientID, you can now paste it in the discord",
                        Duration = 5,
                        Icon = "square",
                    })
                end,
                Variant = "Primary",
            }
        }
    })
else
    -- Optional: Fehlerbehandlung falls der Service nicht verf√ºgbar ist
    warn("RbxAnalyticsService is not available")
end